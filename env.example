# Supabase Configuration
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-goes-here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-goes-here

# App Configuration
APP_NAME=BillNov Mobile
APP_ENV=development # development, staging, production
API_BASE_URL=https://api.example.com

# Feature Flags
ENABLE_ANALYTICS=true
ENABLE_PUSH_NOTIFICATIONS=true

# Authentication
AUTH_PERSISTENT_SESSION=true
AUTH_SESSION_EXPIRY_DAYS=7

# Wallet Configuration
DEFAULT_CURRENCY=NGN
CRYPTO_API_KEY=your-crypto-api-key

# Expense Tracking
EXPENSE_CATEGORIES_LIMIT=20
ENABLE_EXPENSE_ANALYTICS=true

# Storage Configuration
STORAGE_BUCKET=your-storage-bucket-name

# Realtime Configuration
ENABLE_REALTIME=true
REALTIME_TIMEOUT_MS=10000

# Other API Keys
GOOGLE_MAPS_API_KEY=your-google-maps-api-key 